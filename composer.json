{
    "name": "hexlet/code",
    "description": "set of 5 math games",
    "autoload": {
        "psr-4": {
            "Hexlet\\Cod\\": "src/"
        },
        "files": [
            "src/Cli.php",
            "src/index.php",
            "src/games/brain-even.php",
            "src/games/brain-calc.php",
            "src/games/brain-gcd.php",
            "src/games/brain-progression.php",
            "src/games/brain-prime.php"
        ]
    },
    "authors": [
        {
            "name": "vladikKir"
        }
    ],
    "require": {
        "wp-cli/php-cli-tools": "^0.11.17"
    },
    "bin": [
        "bin/brain-games",
        "bin/brain-even",
        "bin/brain-calc",
        "brain/brain-gcd",
        "brain/brain-progression",
        "brain/brain-prime"
    ],
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.7"
    }
}
