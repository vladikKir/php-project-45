{
    "name": "hexlet/code",
    "description": "set of 5 math games",
    "autoload": {
        "files": [
            "src/Cli.php",
            "src/Engine.php",
            "src/Games/BrainEven.php",
            "src/Games/BrainCalc.php",
            "src/Games/BrainGcd.php",
            "src/Games/BrainProgression.php",
            "src/Games/BrainPrime.php"
        ]
    },
    "authors": [
        {
            "name": "vladikKir"
        }
    ],
    "bin": [
        "bin/brain-games",
        "bin/brain-even",
        "bin/brain-calc",
        "bin/brain-gcd",
        "bin/brain-progression",
        "bin/brain-prime"
    ],
    "require": {
        "wp-cli/php-cli-tools": "^0.11.17"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.7",
        "phpstan/phpstan": "^1.9"
    }
}
